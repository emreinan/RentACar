{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RentACar": "Server=(localdb)\\mssqllocaldb;Database=RentACarDb;Trusted_Connection=True;"
  },
  "CacheSettings": {
    "SlidingExpiration": 2
  },
  "SeriLogConfigurations": {
    "FileLogConfiguration": {
      "FolderPath": "/logs/"
    },
    "MsSqlLogConfiguration": {
      "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=RentACarDb;Trusted_Connection=True;",
      "AutoCreateSqlTable": true,
      "TableName": "Logs"
    }
  }
}
